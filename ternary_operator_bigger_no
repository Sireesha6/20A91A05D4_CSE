package conditional_statements;
import java.util.*;
public class Bigger_no_ternary {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter 2 numbers :");
		int a = sc.nextInt(); 
		int b = sc.nextInt();	
		int c = ((a>b) ? a : b);
		System.out.println("bigger number = "+ c);
		
		}

}
