package conditional_statements;

public class Nested_if {

	public static void main(String[] args) {
		int age = 19;
		int weight = 30;
		if(age > 18)
		{
		  if( weight > 40)		  
			  System.out.println("Eligible");
		  else
			  System.out.println("Not Eligible");	  		  
		}
		else
			System.out.println("Age should be more than 18");

	}

}
