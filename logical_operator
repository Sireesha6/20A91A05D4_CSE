package operators;

public class Logical {

	public static void main(String[] args) {
		int a = 10;
		int b = 15;
		int c = 12;
		System.out.println(a<b && a<c); // true
		System.out.println(a>b && a<c); // false
		System.out.println(a<b || a<c); // true
		System.out.println(a<b || a>c); // true
		System.out.println(a>b || a<c); // true
	}

}
